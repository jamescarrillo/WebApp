	  case when trimestre = 4 then 'Trimestre 4'  else
	  case when (trimestre = 1 and anho>='2016') then 'Enero'  else
	  case when (trimestre = 2 and anho>='2016') then 'Febrero'  else
	  case when (trimestre = 3 and anho>='2016') then 'Marzo'  else
	  case when (trimestre = 4 and anho>='2016') then 'Abril'  else
	  case when (trimestre = 5 and anho>='2016') then 'Mayo'  else
	  case when (trimestre = 6 and anho>='2016') then 'Junio'  else
	  case when (trimestre = 7 and anho>='2016') then 'Julio'  else
	  case when (trimestre = 8 and anho>='2016') then 'Agosto'  else
	  case when (trimestre = 9 and anho>='2016') then 'Setiembre'  else
	  case when (trimestre = 10 and anho>='2016') then 'Octubre'  else
	  case when (trimestre = 11 and anho>='2016') then 'Noviembre'  else
	  case when (trimestre = 12 and anho>='2016') then 'Diciembre'

end end end  end end end end end end end end end end end end end format_trimestre
 from web.f00010 where tipo = 1 and anho = $P{p_anho} and trimestre = $P{p_trimestre} ORDER BY apellidos_nombres asc]]>
